<div class="container">
<div class="col-xs-8 clearfix">
    <h2>Users</h2>
    <table class="table table-hover table-inverse">
        <thead>
            <tr> 
              <th>name</th>
              <th colspan="3"></th>
            </tr>
        </thead>
    <% @users.each do |user| %>
        <tbody>
        <tr>
            <td><%=  user.last_name %> <%=  user.first_name %></td>
            <td><%=  user.last_name_kana %> <%=  user.first_name_kana %></td>
            <td><%= link_to "Show", admin_user_path(current_admin.id, user.id)%></td>
        </tr>
        </tbody>
    <% end %>
    </table>
<h1>あああ</h1>
</div>
</div>